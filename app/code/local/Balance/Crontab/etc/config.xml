<config>
    <modules>
        <Balance_Crontab>
            <version>0.0.1</version>
        </Balance_Crontab>
    </modules>
    <global>
        <models>
            <cron>
                <rewrite>
                    <observer>Balance_Crontab_Model_Cron_Observer</observer>
                </rewrite>
            </cron>
            <crontab>
                <class>Balance_Crontab_Model</class>
            </crontab>
        </models>
        <helpers>
            <crontab>
                <class>Balance_Crontab_Helper</class>
            </crontab>
        </helpers>
    </global>
</config>

