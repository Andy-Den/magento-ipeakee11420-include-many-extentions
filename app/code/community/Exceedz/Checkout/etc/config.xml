<?xml version="1.0" encoding="UTF-8"?><config>    <modules>        <Exceedz_Checkout>            <version>1.6.0.1</version>        </Exceedz_Checkout>    </modules>    <global>        <blocks>            <exceedz_checkout>                <class>Exceedz_Checkout_Block</class>            </exceedz_checkout>            <checkout>                <rewrite>                    <onepage>Exceedz_Checkout_Block_Onepage</onepage>                    <!-- remove conflict rewrite don't use cart>Exceedz_Checkout_Block_Cart</cart -->                    <cart_totals>Exceedz_Checkout_Block_Cart_Totals</cart_totals>                    <!-- remove conflict rewrite don't use cart_sidebar>Exceedz_Checkout_Block_Cart_Sidebar</cart_sidebar -->                    <onepage_review_info>Exceedz_Checkout_Block_Onepage_Review_Info</onepage_review_info>                    <cart_item_renderer>Exceedz_Checkout_Block_Cart_Item_Renderer</cart_item_renderer>                    <cart_item_renderer_configurable>Exceedz_Checkout_Block_Cart_Item_Renderer_Configurable</cart_item_renderer_configurable>                </rewrite>            </checkout>        </blocks>        <models>            <checkout>                <rewrite>                    <cart>Exceedz_Checkout_Model_Cart</cart>                </rewrite>            </checkout>        </models>        <resources>            <exceedz_checkout_setup>                <setup>                    <module>Exceedz_Checkout</module>                </setup>                <connection>                    <use>core_setup</use>                </connection>            </exceedz_checkout_setup>            <exceedz_checkout_write>                <connection>                    <use>core_write</use>                </connection>            </exceedz_checkout_write>            <exceedz_checkout_read>                <connection>                    <use>core_read</use>                </connection>            </exceedz_checkout_read>        </resources>    </global>    <frontend>        <routers>            <checkout>                <args>                    <modules>                        <Exceedz_Checkout before="Mage_Checkout">Exceedz_Checkout</Exceedz_Checkout>                    </modules>                </args>            </checkout>        </routers>    </frontend></config>