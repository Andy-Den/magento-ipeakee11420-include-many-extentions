<?xml version="1.0"?>
<config>
    <modules>
        <Exceedz_Newsletter>
            <version>0.1.0</version>
        </Exceedz_Newsletter>
    </modules>
    <frontend>
        <routers>
            <newsletter>
                <use>standard</use>
                <args>
                    <module>Exceedz_Newsletter</module>
                    <frontName>newsletter</frontName>
                </args>
            </newsletter>
        </routers>
    </frontend>
    <global>
        <models>
            <newsletter>
                <rewrite>
                    <subscriber>Exceedz_Newsletter_Model_Subscriber</subscriber>
                </rewrite>
            </newsletter>
        </models>
    </global>
</config>